{
  "name": "webdriverIO-with",
  "version": "1.0.0",
  "description": "webdriverIo",
  "main": "index.js",
  "dependencies": {
    "webdriverio": "^4.13.2",
    "wdio-selenium-standalone-service": "^0.0.10"
  },
  "scripts": {
    "node-inspector": "node-inspector --debug-port 5859 --no-preload",
    "selenium-postinstall": "node node_modules/.bin/selenium-standalone install",
    "selenium-start": "node_modules/.bin/selenium-standalone start",
    "test": "node node_modules/webdriverio/bin/wdio ./test/config/suite.wdio.conf.js",
    "allure-report": "node_modules/.bin/allure generate ./test/reports/allure-results -c && allure open",
    "junit-report": "node_modules/.bin/junit-viewer --results=./test/reports/junit-results --save=junit-results.html && open junit-results.html"
  },
  "keywords": [
    "selenium",
    "appium",
    "mobile",
    "web",
    "testing",
    "webdriverjs",
    "webdriverio",
    "automation",
    "jasmine"
  ],
  "author": "Lambert Egbe",
  "license": "ISC",
  "devDependencies": {
    "grunt": "^1.0.3",
    "grunt-cli": "^1.3.1",
    "grunt-jasmine": "^0.1.0",
    "grunt-webdriver": "^2.0.3",
    "node-uuid": "^1.4.8",
    "selenium-standalone": "^6.15.3",
    "wdio-appium-service": "^0.2.3",
    "wdio-browserstack-service": "^0.1.16",
    "wdio-phantomjs-service": "^0.2.2",
    "wdio-sauce-service": "^0.4.10",
    "wdio-selenium-standalone-service": "^0.0.10"
  },
  "babel": {
    "presets": [
      "env"
    ]
  }
}
